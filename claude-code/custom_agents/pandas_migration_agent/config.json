{
  "agent_name": "EnhancedPandasMigrationAgent",
  "version": "1.0.0",
  "description": "Automated agent for migrating pandas from 0.19.2 to 1.1.5 with backward compatibility",
  "author": "OpenHands Community",
  "supported_pandas_versions": {
    "source": "0.19.2",
    "target": "1.1.5"
  },
  "default_configuration": {
    "auto_rollback_on_failure": true,
    "run_tests_before_migration": true,
    "run_tests_after_migration": true,
    "create_backups": true,
    "strict_compatibility": true,
    "max_retries": 3,
    "test_timeout": 300,
    "parallel_testing": false,
    "custom_import_mappings": {
      "pd.Panel": "aqr.core.panel.Panel",
      "pd.ols": "aqr.stats.ols.OLS",
      "pd.stats.ols.OLS": "aqr.stats.ols.OLS"
    },
    "conda_environments": {
      "0.19.2": "py36-1.1.10",
      "1.1.5": "pandas_115_final"
    }
  },
  "migration_rules": {
    "critical": [
      "replace_pd_panel",
      "replace_pd_ols",
      "replace_pd_stats_ols",
      "replace_ix_with_loc",
      "replace_sort_method",
      "replace_valid_method"
    ],
    "high": [
      "replace_rolling_mean",
      "replace_rolling_sum",
      "replace_rolling_std",
      "replace_expanding_functions"
    ],
    "medium": [
      "replace_as_matrix",
      "replace_get_value",
      "replace_set_value",
      "replace_convert_objects"
    ],
    "low": [
      "replace_categorical_order"
    ]
  },
  "test_frameworks": [
    "pytest",
    "unittest",
    "nose"
  ],
  "exclude_patterns": [
    "__pycache__",
    "\\.pyc$",
    "\\.pyo$",
    "venv/",
    "\\.venv/",
    "env/",
    "\\.env/",
    "\\.git/",
    "\\.tox/",
    "build/",
    "dist/",
    "\\.egg-info/"
  ],
  "report_formats": [
    "markdown",
    "html",
    "json"
  ],
  "dependencies": {
    "runtime": [
      "pandas==0.19.2",
      "pandas==1.1.5",
      "numpy",
      "pytest>=3.0",
      "pytest-cov",
      "pytest-timeout",
      "unittest-xml-reporting"
    ],
    "custom_implementations": {
      "aqr.core.panel": "Custom Panel implementation for 3D data structures",
      "aqr.stats.ols": "Custom OLS regression implementation"
    }
  }
}